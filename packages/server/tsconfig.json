{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2017",
    "noImplicitAny": false,
    "rootDir": "./src",
    "outDir": "./lib",
    "sourceMap": true,
    "forceConsistentCasingInFileNames": true,
    "noUnusedLocals": true,
    "resolveJsonModule": true
  },
  "include": ["./src/**/*"],
  "exclude": ["**/*test_data.ts"]
}
